PREFIX sdo: <https://schema.org/>
PREFIX ont: <https://prez.dev/ont/>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX ex: <http://example.com/>
PREFIX sh: <http://www.w3.org/ns/shacl#>
PREFIX sosa: <http://www.w3.org/ns/sosa/>
PREFIX prez: <https://prez.dev/>

# Defines a shacl path for results such that sosa observations
# are traced back up to publication for which they are about.

ex:Observation
	a sh:PropertyShape ;
	a ont:JenaFTSPropertyShape ;
	sh:path ( sosa:isFeatureOfInterestOf sosa:hasResult ) ;
	sh:name "Observation search path" ;
	dcterms:identifier "observation" ;
	ont:searchPredicate sdo:description ;
.

